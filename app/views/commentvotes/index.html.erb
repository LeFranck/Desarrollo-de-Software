<p id="notice"><%= notice %></p>

<h1>Listing Commentvotes</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>User</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @commentvotes.each do |commentvote| %>
      <tr>
        <td><%= commentvote.type %></td>
        <td><%= commentvote.user_id %></td>
        <td><%= commentvote.comment_id %></td>
        <td><%= link_to 'Show', commentvote %></td>
        <td><%= link_to 'Edit', edit_commentvote_path(commentvote) %></td>
        <td><%= link_to 'Destroy', commentvote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Commentvote', new_commentvote_path %>
